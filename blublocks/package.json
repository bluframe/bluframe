{
  "name": "@bluframe/blublocks",
  "version": "2.3.0",
  "description": "Blu Frame UI Components Library (Blu Blocks)",
  "main": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "author": "Edoardo L'Astorina",
  "license": "MIT",
  "engines": {
    "node": "18"
  },
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/esm/index.js"
    },
    "./blublocks.css": "./lib/css/foundation.css"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && yarn copy:css",
    "build:all": "yarn build && yarn build:cjs",
    "build:ci": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && yarn copy:css",
    "build:cjs": "tsc --project tsconfig.build.cjs.json && tsc-alias -p tsconfig.build.cjs.json",
    "build:cjs:ci": "tsc --project tsconfig.build.cjs.json && tsc-alias -p tsconfig.build.cjs.json",
    "build-storybook": "storybook build",
    "build:types": "tsc --project tsconfig.build.json --declaration && tsc-alias --project tsconfig.build.json",
    "clean:all": "rm -fr build/{*,.tsbuildinfo} || echo 'Build folder already clean!'",
    "clean": "rm -fr build/esm/{*,.tsbuildinfo} || echo 'Build folder already clean!'",
    "clean:cjs": "rm -fr build/cjs/{*,.tsbuildinfo} || echo 'Build folder already clean!'",
    "copy:css": "copyfiles -u 4 src/styles/foundation/css/foundation.css src/styles/foundation/css/foundation.css.map lib/css",
    "flow": "yarn type-check",
    "flow:ci": "yarn type-check:ci",
    "generate:component": "hygen component new",
    "lint": "eslint --ext .ts src/",
    "lint:ci": "eslint --ext .ts --quiet src/",
    "storybook": "storybook dev -p 6006",
    "test": "jest --verbose",
    "test:ci": "jest --coverage --silent",
    "test:coverage": "jest --coverage --verbose",
    "test:local": "jest --onlyChanged --silent",
    "type-check": "tsc --noEmit",
    "type-check:ci": "tsc --noEmit --noEmitOnError"
  },
  "dependencies": {
    "@bluframe/grapple": "^1.0.0",
    "counterup2": "^2.0.2",
    "next": "^14.0.4",
    "polished": "^4.1.4",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-is": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-transition-group": "^4.4.2",
    "strip-markdown": "^5.0.0",
    "styled-components": "^6.1.2",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.2.2",
    "@storybook/addon-docs": "^8.2.2",
    "@storybook/addon-essentials": "^8.2.2",
    "@storybook/addon-links": "^8.2.2",
    "@storybook/manager-api": "^8.2.2",
    "@storybook/nextjs": "^8.2.2",
    "@storybook/react": "^8.2.2",
    "@storybook/source-loader": "^8.2.2",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/copyfiles": "^2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "camelcase": "^6.3.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "enhanced-resolve": "^5.9.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "hygen": "^6.2.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-resolve": "^27.5.0",
    "jest-styled-components": "^7.0.8",
    "jest-watch-typeahead": "^0.6.5",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^11.0.4",
    "react-test-renderer": "^18.0.0",
    "resolve": "^1.22.0",
    "semver": "^7.3.5",
    "storybook": "^8.2.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "webpack": "^5"
  },
  "peerDependencies": {
    "@bluframe/grapple": "^1.0.0",
    "styled-components": "^6.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
